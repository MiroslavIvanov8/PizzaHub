{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Identity": {
      "SignIn": {
        "RequireConfirmedAccount": true
      },
      "Password": {
        "RequireDigit": false,
        "RequireUppercase": false,
        "RequireNonAlphanumeric": false
      }
    }
  }
}